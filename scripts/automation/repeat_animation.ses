() {
    ~ project p = $SE.get_project();
    ~ int fc = p.get_frame_count();
    ~ layer[] ls = p.get_layers();

    p.set_frame_index(fc - 1);

    for (int f = 0; f < fc; f++)
        p.add_frame();

    for (int l = 0; l < #|ls; l++) {
        for (int f = 0; f < fc; f++) {
            ~ image frame = ls[l].get_frame(f);
            ls[l].set_frame(f + fc, frame);
        }
    }
}
